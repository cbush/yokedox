{
  "name": "yokedox",
  "description": "A yoked ox",
  "version": "0.0.0",
  "author": "Chris Bush @cbush",
  "bin": {
    "yokedox": "./bin/run"
  },
  "bugs": "https://github.com/cbush/yokedox/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "tslib": "^1.13.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/jest": "^26.0.13",
    "@types/node": "^10.17.32",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "globby": "^10.0.2",
    "jest": "^26.4.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/cbush/yokedox",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "yokedox",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "cbush/yokedox",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "coverage": "jest --coverage",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
